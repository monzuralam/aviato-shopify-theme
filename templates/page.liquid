<section class="page-header">
	<div class="container">
		<div class="row">
			<div class="col-md-12">
				<div class="content">
					<h1 class="page-name">{{ page.title }}</h1>
					<ol class="breadcrumb">
						<li><a href="/">Home</a></li>
						<li class="active">{{ page.title | downcase }}</li>
					</ol>
				</div>
			</div>
		</div>
	</div>
</section>
<section class="section">
    <div class="container">
        <div class="row">
            <h2>{{ page.title }}</h2>
            <div>{{ page.content }}</div>
        </div>
    </div>
</section>