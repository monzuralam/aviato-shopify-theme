<div class="section video-testimonial overly-white text-center mt-50">
	<div class="container">
		<div class="row">
			<div class="col-md-12">
				<h2>{{ section.settings.testimonial-video-title }}</h2>
                {%- if section.settings.testimonial-video-link != blank -%}
                <a class="play-icon" href="{{ section.settings.testimonial-video-link }}" data-toggle="lightbox">
                    <i class="tf-ion-ios-play"></i>
                </a>
                {%- else -%}
                <a class="play-icon" href="https://youtu.be/7KusECide24" data-toggle="lightbox">
					<i class="tf-ion-ios-play"></i>
				</a>
                {%- endif -%}
			</div>
		</div>
	</div>
</div>
{%- style -%}
    .video-testimonial{
        {%- if section.settings.testimonial-bg-image != blank -%}
        background-image: url( '{{ section.settings.testimonial-bg-image | image_url: width: 1920 }}' );
        {%- else -%}
        background-image: url( 'data:image/png;base64,' );
        {%- endif -%}
        background-size: cover;
    }
{%- endstyle -%}
{% schema %}
{
    "name"      : "Testimonial Video",
    "settings"  : [
        {
            "type"      : "text",
            "id"        : "testimonial-video-title",
            "default"   : "Video presentation",
            "label"     : "Title"
        },
        {
            "type"      : "url",
            "id"        : "testimonial-video-link",
            "label"     : "Video URL"
        },
        {
            "type"      : "image_picker",
            "id"        : "testimonial-bg-image",
            "label"     : "Background Image"
        }
    ],
    "presets"   : [
        {
            "name"  : "Testimonial Video"
        }
    ]
}
{% endschema %}