<section class="product-category section">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="title text-center">
                    <h2>{{ section.settings.product-cat-title }}</h2>
                </div>
            </div>
            <div class="col-md-6">
                <div class="category-box">
                    <a href="{{ section.settings.product_cat_1_link }}">
                        {%- if section.settings.product_cat_1_img != blank -%}
                            {{ section.settings.product_cat_1_img | image_url: width: 555 | image_tag: class: 'img-responsive', loading: 'lazy' }}
                        {%- else -%}
                        <img src="data:image/png;base64," alt="img" />
                        {%- endif -%}
                        <div class="content">
                            <h3>{{ section.settings.product_cat_1_title }}</h3>
                            <p>{{ section.settings.product_cat_1_desc }}</p>
                        </div>
                    </a>
                </div>
                <div class="category-box">
                    <a href="{{ section.settings.product_cat_2_link }}">
                        {%- if section.settings.product_cat_2_img != blank -%}
                            {{ section.settings.product_cat_2_img | image_url: width: 555 | image_tag: class: 'img-responsive', loading: 'lazy' }}
                        {%- else -%}
                            <img src="data:image/png;base64," alt="img" />
                        {%- endif -%}
                        <div class="content">
                            <h3>{{ section.settings.product_cat_2_title }}</h3>
                            <p>{{ section.settings.product_cat_2_desc }}</p>
                        </div>
                    </a>
                </div>
            </div>
            <div class="col-md-6">
                <div class="category-box category-box-2">
                    <a href="{{ section.settings.product_cat_3_link }}">
                        {%- if section.settings.product_cat_3_img != blank -%}
                            {{ section.settings.product_cat_3_img | image_url: width: 555 | image_tag: class: 'img-responsive', loading: 'lazy' }}
                        {%- else -%}
                            <img src="data:image/png;base64," alt="img" />
                        {%- endif -%}
                        <div class="content">
                            <h3>{{ section.settings.product_cat_3_title }}</h3>
                            <p>{{ section.settings.product_cat_3_desc }}</p>
                        </div>
                    </a>
                </div>
            </div>
        </div>
    </div>
</section>
{% schema %}
{
    "name"      : "Product Category",
    "class"     : "page-section",
    "settings"  : [
        {
            "type"      : "header",
            "content"   : "Product Category"
        },
        {
            "type"      : "text",
            "id"        : "product-cat-title",
            "default"   : "Product Category",
            "label"     : "Title"
        },
        {
            "type"      : "header",
            "content"   : "Category 1"
        },
        {
            "type"      : "image_picker",
            "id"        : "product_cat_1_img",
            "label"     : "Image"
        },
        {
            "type"      : "text",
            "id"        : "product_cat_1_title",
            "default"   : "Clothes Sales",
            "label"     : "Title"
        },
        {
            "type"      : "textarea",
            "id"        : "product_cat_1_desc",
            "default"   : "Shop New Season Clothing",
            "label"     : "Title"
        },
        {
            "type"      : "url",
            "id"        : "product_cat_1_link",
            "label"     : "Link"
        },
        {
            "type"      : "header",
            "content"   : "Category 2"
        },
        {
            "type"      : "image_picker",
            "id"        : "product_cat_2_img",
            "label"     : "Image"
        },
        {
            "type"      : "text",
            "id"        : "product_cat_2_title",
            "default"   : "Smart Casuals",
            "label"     : "Title"
        },
        {
            "type"      : "textarea",
            "id"        : "product_cat_2_desc",
            "default"   : "Get Wide Range Selection",
            "label"     : "Title"
        },
        {
            "type"      : "url",
            "id"        : "product_cat_2_link",
            "label"     : "Link"
        },
        {
            "type"      : "header",
            "content"   : "Category 3"
        },
        {
            "type"      : "image_picker",
            "id"        : "product_cat_3_img",
            "label"     : "Image"
        },
        {
            "type"      : "text",
            "id"        : "product_cat_3_title",
            "default"   : "Jewellery",
            "label"     : "Title"
        },
        {
            "type"      : "textarea",
            "id"        : "product_cat_3_desc",
            "default"   : "Special Design Comes First",
            "label"     : "Title"
        },
        {
            "type"      : "url",
            "id"        : "product_cat_3_link",
            "label"     : "Link"
        }
    ],
    "presets"   : [
        {
            "name"  : "Product Category"
        }
    ]
}
{% endschema %}